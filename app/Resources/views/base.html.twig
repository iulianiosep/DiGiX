<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}|My Legacy Box{% endblock %}</title>

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />


        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
        <meta name="description" content="">
        <meta name="author" content="">
        <link rel="icon" href="../../favicon.ico">



        <!-- Bootstrap core CSS -->
        {% block stylesheets %}
            {% stylesheets 'bundles/digix/css/*' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset('bundles/digix/css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/digix/css/styles.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/digix/css/font-awesome.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/digix/css/modal.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/digix/css/mainCSS.css') }}" />
            {% endstylesheets %}
        {% endblock %}

        <!-- Just for debugging purposes. Don't actually copy these 2 lines! -->
        <!--[if lt IE 9]><script src="../../assets/js/ie8-responsive-file-warning.js"></script><![endif]-->

        


    </head>
    <body>
        <div id="header">
            {% block header %}      
            <nav class="navbar navbar-fixed-top navbar-inverse">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">DiGix</a>
                    </div>
                    <div id="navbar" class="collapse navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li class="active"><a href="{{path('digix_wall')}}">Tag them</a></li>
                            <li><a href="{{path('search_controller')}}">Search them</a></li>
                            <li><a href="{{path('box_page')}}">My Box</a></li>
                            <li><a href="{{path('about_page')}}">About</a></li>
                            <li><a href="{{path('contact_page')}}">Support</a></li>
                        </ul>

                        <div class="social">
                            <a class="facebook-hover social-slide" href="#openModal"></a>
                            <a class="twitter-hover social-slide"  href="#"></a>
                            <a class="google-hover social-slide"  href="#"></a>
                            <a class="instagram-hover social-slide"  href="#"></a>
                            <a class="tumblr-hover social-slide"  href="#"></a>
                        </div>



                    </div><!-- /.nav-collapse -->
                </div><!-- /.container -->

            </nav><!-- /.navbar -->
            {% endblock %}
        </div>
        {% block body %}   {% endblock %}

        {% block javascripts %}
            {% javascripts 'bundles/digix/js/*' %}
                <script src="{{ asset('bundles/digix/js/ie-emulation-modes-warning.js') }}"></script>
                <script src="{{ asset('bundles/digix/js/bootstrap.min.js') }}"></script>
                <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
                <script src="{{ asset('bundles/digix/js/ie10-viewport-bug-workaround.js') }}"></script>
                <script src="{{ asset('bundles/digix/js/offcanvas.js') }}"></script>
                <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
                <!--[if lt IE 9]>
                <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
                <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>-->
            {% endjavascripts %}
        {% endblock %}

    </body>
</html>
